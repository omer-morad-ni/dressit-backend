{
  "name": "nest-typescript-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start": "cross-env NODE_ENV=production && node dist/main",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "echo 'No tests this time' && exit 0",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "echo 'No e2e tests implemented yet.'"
  },
  "dependencies": {
    "@nestjs/common": "7.0.11",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "7.0.11",
    "@nestjs/platform-express": "7.0.11",
    "@nestjs/typeorm": "7.0.0",
    "mysql": "2.18.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.5.5",
    "typeorm": "0.2.24"
  },
  "devDependencies": {
    "@nestjs/cli": "7.1.5",
    "@nestjs/schematics": "7.0.0",
    "@nestjs/testing": "7.0.11",
    "@types/express": "4.17.6",
    "@types/node": "^7.10.9",
    "@types/supertest": "2.0.9",
    "@types/ws": "7.2.4",
    "@typescript-eslint/eslint-plugin": "3.0.0",
    "@typescript-eslint/parser": "3.0.0",
    "dotenv": "^8.2.0",
    "eslint": "7.0.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "26.0.1",
    "lint-staged": "^10.2.4",
    "prettier": "2.0.5",
    "supertest": "4.0.2",
    "ts-jest": "26.0.0",
    "ts-loader": "7.0.4",
    "ts-node": "8.10.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.7.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix"
    ],
    "*.{md,yml,yml,json}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/omer-morad-ni/dressit-backend.git"
  }
}
