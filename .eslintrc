{
  "plugins": [
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "printWidth": 120,
        "trailingComma": "es5"
      }
    ],
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": true
      }
    ],
    "no-unused-expressions": 0,
    "one-var": 0,
    "no-underscore-dangle": [
      0,
      {
        "allow": []
      }
    ],
    "no-plusplus": 0,
    "func-names": 0,
    "no-use-before-define": 0,
    "no-useless-constructor": "off",
    "no-console": 0,
    "global-require": 0,
    "jsx-a11y/label-has-for": 0,
    "class-methods-use-this": 0,
    "no-empty-function": "off",
    "new-cap": "off",
    "import/extensions": 0,
    "camelcase": "off",
    "no-return-await": 0,
    "consistent-return": 0,
    "import/prefer-default-export": 0
  },
  "env": {
    "jest": true,
  }
}
